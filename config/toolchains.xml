<?xml version="1.0" encoding="UTF8"?>
<toolchains>
	<!-- JDK toolchains -->
	<toolchain>
		<type>jdk</type>
		<provides>
		  <version>1.7</version>
		</provides>
		<configuration>
		  <jdkHome>/usr/lib/jvm/java-7-openjdk-amd64</jdkHome>
		</configuration>
	</toolchain>
	<toolchain>
		<type>jdk</type>
		<provides>
		  <version>1.8</version>
		</provides>
		<configuration>
		  <jdkHome>/usr/lib/jvm/java-8-oracle</jdkHome>
		</configuration>
	</toolchain>
</toolchains>
